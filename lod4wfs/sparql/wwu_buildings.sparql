{
"name":"http://sparql.lod4wfs.de/wwu_buildings",
"title":"Geb�de der Universit� M�nster",
"abstract":"no abstract",
"keywords":"no keywords",
"geometryVariable":"?wkt",
"endpoint":"http://data.uni-muenster.de:8080/openrdf-sesame/repositories/lodumhbz2",
"query":"PREFIX dbp-ont:<http://dbpedia.org/ontology/>
PREFIX wgs84_pos:<http://www.w3.org/2003/01/geo/wgs84_pos#>
PREFIX foaf:<http://xmlns.com/foaf/0.1/>
PREFIX vcard:<http://www.w3.org/2006/vcard/ns#>

SELECT ?building ?name ?street ?region ?country  (concat('POINT(', ?long, ' ', ?lat, ')') AS ?wkt) 
WHERE {
	?building ?p dbp-ont:building .
	?building wgs84_pos:lat ?lat .
	?building wgs84_pos:long ?long .
	?building foaf:name ?name .	
	?building vcard:adr ?address .	
	?address  vcard:street-address ?street .
	?address  vcard:region ?region	.
	?address  vcard:country-name ?country	.
}"
}